//My 8th c++ program.
//Take 2 number from the user as input and print there sum using c++

#include<iostream>
using namespace std;
int main(){
int num1, num2;
cout<<"Enter Two Numbers."<<endl;
cout<<"First Number : ";
cin>>num1;
cout<<"Second Number: ";
cin>>num2;

cout<<"Sum of Both the Numbers is = "<<num1+num2;

return 0;
}
